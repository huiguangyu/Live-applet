.page {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: #f9f9f9;
  font-size: 28rpx;
}

.content {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}

.agreement {
  color: #15b111;
}

.btn {
  padding: 8rpx 20rpx;
  color: #ffffff;
  background-color: #15b111;
  border-radius: 20px;
  margin: auto;
  text-align: center;
   margin-top: 40rpx;
}
