.page {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #f9f9f9;
}

.version {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.title {
  font-size: 40rpx;
  text-align: center;
  font-weight: 600;
  margin-bottom: 20rpx;
}

.pictrue {
  width: 200px;
  height: 100px;
  border-radius: 4px;
}

.text {
  font-size: 28rpx;
  text-align: center;
}